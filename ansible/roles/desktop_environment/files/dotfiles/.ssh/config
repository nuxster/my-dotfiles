Host * !notabug.org
    IdentitiesOnly=yes
    RemoteCommand TERM=xterm $SHELL
    RequestTTY yes

Host git.loc-rep1
    HostName git.loc
    User git
    IdentityFile ~/.ssh/git-username-rep1

Host git.loc-rep2
    HostName git.loc
    User git
    IdentityFile ~/.ssh/git-username-rep2

Host notabug.org
    Hostname notabug.org
    IdentityFile ~/.ssh/notabug-username
